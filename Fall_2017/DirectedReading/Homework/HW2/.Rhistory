prob_norm_odd <- function(n){
norm.df <- data.frame(rnorm(n))
norm.round.df <- round(norm.df)
colnames(norm.round.df) <- c("Binary")
norm.round.binary.df <- (1 *(norm.round.df%%2!=0))
return(1 - (sum(norm.round.binary.df)/n))
}
